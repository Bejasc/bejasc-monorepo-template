{
	"compilerOptions": {
		"target": "ESNext",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"noImplicitAny": false
	}
}
